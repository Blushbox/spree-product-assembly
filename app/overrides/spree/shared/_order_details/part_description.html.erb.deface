<!-- insert_bottom 'td[data-hook=order_item_description]' -->
<% if item.variant.assembly? %>
  <ul class='assembly_parts'>
    <% item.variant.parts.each do |v| %>
    <li>(<%= item.variant.count_of(v) %>)  <%= link_to v.name, product_path(v.product) %>  (<%= v.sku %>)</li>
    <% end %>
  </ul>
<% end %>
